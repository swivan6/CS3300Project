<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<% @projects.each do |project| %>
   <div class="row">
    <div class="col-sm-4 my-4">
      <div class="card">
        <div class="card-body">
          <h2 class="card-title"><%= project.name %></h2>
          <p class="card-text"><%= project.description %></p>
        </div>        
      </div>
    </div>
  </div>
<% end %>
 
<br>

<%= link_to 'New Project', new_project_path %>
